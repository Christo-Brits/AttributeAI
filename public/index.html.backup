    <!-- Google Analytics 4 (GA4) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-BDZZKFKYDV"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      
      // Configure GA4 with enhanced attribution and commerce tracking
      gtag('config', 'G-BDZZKFKYDV', {
        // Enhanced attribution settings for marketing platform
        allow_enhanced_conversions: true,
        allow_google_signals: true,
        send_page_view: true,
        
        // Custom tracking for SaaS platform
        custom_map: {
          'custom_parameter_1': 'tool_used',
          'custom_parameter_2': 'subscription_tier',
          'custom_parameter_3': 'analysis_type'
        },
        
        // Privacy and compliance settings
        anonymize_ip: true,
        cookie_flags: 'secure;samesite=strict',
        cookie_expires: 63072000, // 2 years
        
        // Performance monitoring
        page_title: document.title,
        page_location: window.location.href,
        
        // Attribution platform specific settings
        linker: {
          domains: ['attributeai.app', 'www.attributeai.app']
        }
      });
      
      // Track initial platform load
      gtag('event', 'platform_load', {
        'event_category': 'engagement',
        'event_label': 'initial_load',
        'value': 1
      });
    </script>
    
