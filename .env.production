# Production Environment Variables for Netlify
# These are used when the app is deployed

# React App API URL - Points to your backend
REACT_APP_API_URL=/api

# Build Configuration
CI=false
GENERATE_SOURCEMAP=false
SKIP_PREFLIGHT_CHECK=true
DISABLE_ESLINT_PLUGIN=true
ESLINT_NO_DEV_ERRORS=true

# Feature Flags
REACT_APP_ENABLE_AI_CHAT=true
REACT_APP_ENABLE_KEYWORD_INTELLIGENCE=true
REACT_APP_ENABLE_CONTENT_GENERATION=true
REACT_APP_DEMO_MODE=false

# API Keys are set in Netlify Dashboard
# DO NOT put actual keys here