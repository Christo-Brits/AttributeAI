# AttributeAI Environment Configuration
# Copy this file to .env and add your actual API keys

# =============================================================================
# AI SERVICE PROVIDERS
# =============================================================================

# OpenAI (for GPT-4, ChatGPT)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_OPENAI_ORG_ID=your_openai_org_id_here

# Anthropic Claude
REACT_APP_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI (Gemini)
REACT_APP_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# =============================================================================
# WEB SEARCH & RESEARCH APIS
# =============================================================================

# Brave Search (for web search functionality)
REACT_APP_BRAVE_SEARCH_API_KEY=your_brave_search_api_key_here

# Google Custom Search (alternative web search)
REACT_APP_GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here
REACT_APP_GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id_here

# Perplexity AI (for research-backed content)
REACT_APP_PERPLEXITY_API_KEY=your_perplexity_api_key_here

# =============================================================================
# SEO & MARKETING APIS
# =============================================================================

# SEMrush (for keyword research and competitor analysis)
REACT_APP_SEMRUSH_API_KEY=your_semrush_api_key_here

# Ahrefs (for SEO data and backlink analysis)
REACT_APP_AHREFS_API_KEY=your_ahrefs_api_key_here

# Moz (for domain authority and SEO metrics)
REACT_APP_MOZ_ACCESS_ID=your_moz_access_id_here
REACT_APP_MOZ_SECRET_KEY=your_moz_secret_key_here

# Google Search Console (for site performance data)
REACT_APP_GOOGLE_SEARCH_CONSOLE_CLIENT_ID=your_gsc_client_id_here
REACT_APP_GOOGLE_SEARCH_CONSOLE_CLIENT_SECRET=your_gsc_client_secret_here

# =============================================================================
# WEATHER API (for attribution correlation)
# =============================================================================

# OpenWeatherMap
REACT_APP_WEATHER_API_KEY=your_openweather_api_key_here

# WeatherAPI.com (alternative weather service)
REACT_APP_WEATHERAPI_KEY=your_weatherapi_key_here

# =============================================================================
# ANALYTICS & TRACKING
# =============================================================================

# Google Analytics 4
REACT_APP_GA4_MEASUREMENT_ID=your_ga4_measurement_id_here

# Google Ads (for attribution tracking)
REACT_APP_GOOGLE_ADS_CUSTOMER_ID=your_google_ads_customer_id_here
REACT_APP_GOOGLE_ADS_DEVELOPER_TOKEN=your_google_ads_developer_token_here

# Facebook/Meta Pixel
REACT_APP_META_PIXEL_ID=your_meta_pixel_id_here

# =============================================================================
# CONTENT & CMS INTEGRATION
# =============================================================================

# WordPress (for content publishing)
REACT_APP_WORDPRESS_API_URL=your_wordpress_site_url_here
REACT_APP_WORDPRESS_USERNAME=your_wordpress_username_here
REACT_APP_WORDPRESS_APP_PASSWORD=your_wordpress_app_password_here

# Webflow (alternative CMS)
REACT_APP_WEBFLOW_API_TOKEN=your_webflow_api_token_here
REACT_APP_WEBFLOW_SITE_ID=your_webflow_site_id_here

# =============================================================================
# DATABASE & STORAGE
# =============================================================================

# MongoDB (for storing analysis results)
REACT_APP_MONGODB_CONNECTION_STRING=your_mongodb_connection_string_here

# Supabase (alternative database)
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AWS S3 (for file storage)
REACT_APP_AWS_ACCESS_KEY_ID=your_aws_access_key_here
REACT_APP_AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
REACT_APP_AWS_BUCKET_NAME=your_aws_bucket_name_here
REACT_APP_AWS_REGION=us-east-1

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================

# Environment
REACT_APP_NODE_ENV=development

# API Base URLs
REACT_APP_API_BASE_URL=http://localhost:3001
REACT_APP_FRONTEND_URL=http://localhost:3000

# Feature Flags
REACT_APP_ENABLE_SEO_ANALYSIS=true
REACT_APP_ENABLE_CONTENT_GENERATION=true
REACT_APP_ENABLE_WEATHER_CORRELATION=true
REACT_APP_ENABLE_REAL_TIME_TRACKING=true

# Rate Limiting
REACT_APP_API_RATE_LIMIT=100
REACT_APP_SEARCH_RATE_LIMIT=50

# Debug Mode
REACT_APP_DEBUG_MODE=false
REACT_APP_VERBOSE_LOGGING=false

# =============================================================================
# SECURITY & AUTHENTICATION
# =============================================================================

# JWT Secret (for user authentication)
JWT_SECRET=your_jwt_secret_key_here

# Encryption Key (for sensitive data)
ENCRYPTION_KEY=your_encryption_key_here

# CORS Origins (for production)
REACT_APP_ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# =============================================================================
# THIRD-PARTY INTEGRATIONS
# =============================================================================

# Zapier (for workflow automation)
REACT_APP_ZAPIER_WEBHOOK_URL=your_zapier_webhook_url_here

# Slack (for notifications)
REACT_APP_SLACK_BOT_TOKEN=your_slack_bot_token_here
REACT_APP_SLACK_CHANNEL_ID=your_slack_channel_id_here

# Email Service (for reports)
REACT_APP_SENDGRID_API_KEY=your_sendgrid_api_key_here
REACT_APP_FROM_EMAIL=noreply@yourdomain.com

# =============================================================================
# NOTES
# =============================================================================
# 
# 1. Never commit this file with real API keys to version control
# 2. Add .env to your .gitignore file
# 3. Use REACT_APP_ prefix for variables accessible in React frontend
# 4. Variables without REACT_APP_ prefix are for backend/server use only
# 5. Consider using environment-specific files (.env.development, .env.production)
# 6. For production, use proper secrets management (AWS Secrets Manager, etc.)
# 7. Some services may require additional setup (OAuth flows, webhooks, etc.)
#
# API Key Sources:
# - OpenAI: https://platform.openai.com/api-keys
# - Anthropic: https://console.anthropic.com/
# - Brave Search: https://api.search.brave.com/
# - Google AI: https://makersuite.google.com/app/apikey
# - Perplexity: https://www.perplexity.ai/settings/api
# - SEMrush: https://www.semrush.com/api-documentation/
# - Ahrefs: https://ahrefs.com/api/documentation
# - OpenWeather: https://openweathermap.org/api
#
