# AttributeAI Environment Configuration
# Copy this to .env and replace with your actual values

# Supabase Configuration (Required for Google Sign-In)
REACT_APP_SUPABASE_URL=https://your-project-ref.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your_anon_key_here

# OAuth Configuration (Optional - for additional providers)
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_GITHUB_CLIENT_ID=your_github_client_id
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id

# Authentication URLs
REACT_APP_AUTH_CALLBACK_URL=http://localhost:3000/auth/callback
REACT_APP_REDIRECT_URL=http://localhost:3000/dashboard
REACT_APP_SITE_URL=https://leafy-biscotti-c87e93.netlify.app

# Development Settings
REACT_APP_NODE_ENV=development
